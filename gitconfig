[core]
	editor = vim
	autocrlf = input
	safecrlf = true
[color]
	diff = true
	status = true
	branch = true
	ui = true
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
